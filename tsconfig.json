{
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"target": "es2020",
		"module": "es2020",
		"moduleResolution": "node",
		"lib": [
			"es2017"
		],
		"declaration": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"importHelpers": true
	}
}